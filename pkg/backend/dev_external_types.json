{
    "types": [
        {
            "type": "registry",
            "kind": "dockerhub",
            "service-url": "http://local.codefresh.io:10001",
            "uri-template": "registry:dockerhub:{{namespace}}:{{name}}:{{action}}",
            "uri-regex": "^registry:dockerhub:[a-z0-9_-]+:[a-z0-9_-]+:push$",
            "config": [
                {
                    "name": "namespace",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "action",
                    "type": "list:push",
                    "validator": "^(push)$",
                    "required": true
                }
            ],
            "filters": [
                {
                    "name": "TAG",
                    "type": "string",
                    "validator": "^.+$"
                }
            ]
        },
        {
            "type": "git",
            "kind": "github",
            "service-url": "http://local.codefresh.io:10003",
            "uri-template": "git:github:{{owner}}:{{name}}:{{action}}",
            "uri-regex": "^git:github:.+:.+:push$",
            "config": [
                {
                    "name": "owner",
                    "type": "string",
                    "validator": "^.+$",
                    "required": true,
                    "filter": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^.+$",
                    "required": true
                },
                {
                    "name": "action",
                    "type": "list:push",
                    "validator": "^(push)$",
                    "required": true
                }
            ],
            "filters": [
                {
                    "name": "BRANCH",
                    "type": "string",
                    "validator": "^.+$"
                }
            ]
        }
    ]
}