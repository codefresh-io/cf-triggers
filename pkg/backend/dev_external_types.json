{
    "types": [
        {
            "type": "registry",
            "kind": "dockerhub",
            "service-url": "http://local.codefresh.io:10001",
            "uri-template": "registry:dockerhub:{{namespace}}:{{name}}:{{action}}",
            "uri-regex": "^registry:dockerhub:[a-z0-9_-]+:[a-z0-9_-]+:push$",
            "config": [
                {
                    "name": "namespace",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "action",
                    "type": "list",
                    "options": {
                        "Push Image": "push"
                    },
                    "validator": "^(push)$",
                    "required": true
                }
            ],
            "filters": [
                {
                    "name": "tag",
                    "type": "string",
                    "validator": "^.+$"
                }
            ]
        },
        {
            "type": "custom",
            "kind": "custom",
            "service-url": "http://local.codefresh.io:10004",
            "uri-template": "custom:{{name}}",
            "uri-regex": "^custom:.+$",
            "config": [
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^.+$",
                    "required": true
                }
            ],
            "filters": []
        }
    ]
}