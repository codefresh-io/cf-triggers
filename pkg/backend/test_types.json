{
    "types": [{
            "type": "registry",
            "kind": "dockerhub",
            "service-url": "http://localhost:8080",
            "uri-template": "index.docker.io:{{namespace}}:{{name}}:push",
            "uri-regex": "^index\\.docker\\.io:[a-z0-9_-]+:[a-z0-9_-]+:push$",
            "config": [{
                    "name": "namespace",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                }
            ]
        },
        {
            "type": "registry",
            "kind": "cfcr",
            "service-url": "http://localhost:8080",
            "uri-template": "r.cfcr.io:{{namespace}}:{{name}}:push",
            "uri-regex": "^r\\.cfcr\\.io:[a-z0-9_-]+:[a-z0-9_-]+:push$",
            "config": [{
                    "name": "namespace",
                    "type": "string",
                    "validator": "^[Ð°-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                }
            ]
        }
    ]
}