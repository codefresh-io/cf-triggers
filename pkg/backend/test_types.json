{
    "types": [{
            "type": "registry",
            "kind": "dockerhub",
            "service-url": "http://localhost:8080",
            "uri-template": "registry:dockerhub:{{namespace}}:{{name}}:push",
            "uri-regex": "^registry:dockerhub:[a-z0-9_-]+:[a-z0-9_-]+:push$",
            "config": [{
                    "name": "namespace",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                }
            ]
        },
        {
            "type": "registry",
            "kind": "cfcr",
            "service-url": "http://localhost:8080",
            "uri-template": "registry:cfcr:{{namespace}}:{{name}}:push",
            "uri-regex": "^registry:cfcr:[a-z0-9_-]+:[a-z0-9_-]+:push$",
            "config": [{
                    "name": "namespace",
                    "type": "string",
                    "validator": "^[Ð°-z0-9_-]+$",
                    "required": true
                },
                {
                    "name": "name",
                    "type": "string",
                    "validator": "^[a-z0-9_-]+$",
                    "required": true
                }
            ]
        }
    ]
}